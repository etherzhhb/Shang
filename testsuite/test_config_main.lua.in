// Block Ram $(num)
reg   [$(datawidth - 1):0]   bram$(num)out_r;
assign bram$(num)out = bram$(num)out_r;
reg   [$(datawidth - 1):0]  mem$(num)[0:$(2^addrwidth-1)];

#if filename ~= [[]] then
